<script type="text/javascript">
	function checkOlMaps() {// Check if the browser supports SVG. If not reload the page with flag to not use OpenLayers
		if (document.getElementById("noOL") || !document.getElementById("sidemap")) {
			return;
		}
		var supportsSVG = false;
		try {
			var svg = document.createElementNS("http://www.w3.org/2000/svg", 'svg');
			supportsSVG = typeof svg.createSVGPoint == 'function';
		} catch (e) {
		}
		if (supportsSVG) {
			olSmallMapInit();
		} else {
			var url = window.location.href;
			if (url.indexOf("&useOLMaps=Y") > -1) {
				url = url.replace("&useOLMaps=Y", "");
			}
			if (url.indexOf("&useOLMaps=y") > -1) {
				url = url.replace("&useOLMaps=y", "");
			}
			url = url + "&useOLMaps=N"
			window.location.assign = url;
		}
	}
</script>


