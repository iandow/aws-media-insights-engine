var UnifSearchEngine = {
		showProgressBar : function (msg,elToHide) {
			animationStarted = false;
			showProgressBar( "contentProgressBar", msg, "", "contentArea" );
		}
} 

function getMaximumWindowDate()
{
	var dateMaxWindowEle = document.getElementById("dateMaxWindow");
	if (dateMaxWindowEle != null){
		return new Date(dateMaxWindowEle.value);
	}	
	else
	{
		today = new Date();
		today.setHours(0,0,0,0);
	
		lastDate = new Date(today);
		lastDate.setDate(1*(today.getDate())+(365));
		return lastDate;
	} 
}

function changeDate(matrixStartDate, date, dayUseStayAdjust, url)
{
	if ($j("#calendar").length == 0) {
    setDepartureDate(dayUseStayAdjust);
		return;
	}

	lastDate = getMaximumWindowDate();
	newd = new Date(date);
  	//Handle special case for multiple language, Ex: mer. 8 déc. 2021
  	if('Invalid Date' == newd){
		try{
			newd = new Date(changeLocaleDateString2EnglishDateString(date));
		}catch(e){}
  	}
	if (newd >= today && newd <= lastDate) { // if the date requested is beyond today + 1 year do nothing
		msd = new Date(matrixStartDate);
		newOffset = Math.round((newd-msd)/(1000*60*60*24));
		if (newOffset<0 || newOffset>=14) {
			url = url + '&arvdate=' + asDoubleDigit(newd.getMonth()+1)+"/"+asDoubleDigit(newd.getDate())+"/"+newd.getFullYear() +
									'&lengthOfStay=' + document.getElementById('lengthOfStay').value;
			window.location.href = url;
		}
		var hasError = false;
		var complexstatus = "";
 		var countAvail = 0;
 		var countInLottery = 0;
		for (var i = 0; i < 14; i++) {
		  	var matrixDay = document.getElementById('avail' + (i + 1));
		  	addClassName (matrixDay,'slct',false);
			if (i >= newOffset && i < (newOffset + 1*document.getElementById('lengthOfStay').value)) {
		  		addClassName (matrixDay,'slct',true);
		  		var status = null;
				var considerAvail = false;
				if ( hasClassName(matrixDay,'a') || hasClassName(matrixDay,'b')) { 
					status = 'a'; considerAvail = true;
					countAvail =  countAvail + 1;
				} else if ( hasClassName(matrixDay,'l') ) {
					status = 'l'; considerAvail = true;
					countInLottery = countInLottery  + 1;
				} else {
					hasError = true;
				}
				complexstatus = complexstatus + status;
				
			}
				
			if (i < newOffset && i >= (newOffset + 1*document.getElementById('lengthOfStay').value))  {
		  		addClassName (matrixDay,'/',false);
				//matrixDay.className = 'status a';
			}
		}
		if (countAvail > 0 && countInLottery > 0){
			hasError = true;
			countAvail = 0;
			countInLottery = 0;
		}
	    setDepartureDate(dayUseStayAdjust);
	    if (document.getElementById('AlternativeSites')) {
			reloadSitesDropdown(newd);
	    }

	    
		if (document.getElementById('selStatus')) {
			document.getElementById('selStatus').value=complexstatus;
		}
		
		addClassName (document.getElementById('lengthOfStay'),'error',hasError);
		addClassName (document.getElementById('arrivaldate'),'error',hasError);
	
		if (document.getElementById('matrixHasError'))
			document.getElementById('matrixHasError').value = hasError?"true":"false";
	}
	else {
		document.getElementById('matrixHasError').value = "true";
		if (document.getElementById('matrixprogresspopup'))
			document.getElementById('matrixprogresspopup').style.display = "none";
		if (document.getElementById('progresspopup'))
			document.getElementById('progresspopup').style.display ="none";
	}
}
function changeNights(matrixStartDate, date, dayUseStayAdjust)
{
	if ($j("#calendar").length == 0) {
		setDepartureDate(dayUseStayAdjust);
		return;
	}
  msd = new Date(matrixStartDate);
  newd = new Date(document.getElementById('arrivaldate').value);
  //Handle special case for multiple language, Ex: mer. 8 déc. 2021
  if('Invalid Date' == newd){
		try{
			newd = new Date(changeLocaleDateString2EnglishDateString(document.getElementById('arrivaldate').value));
		}catch(e){}
  }
  lastDate = getMaximumWindowDate();
  today = new Date();
  today.setHours(0,0,0,0);
  
  if (newd >= today && newd <= lastDate) { // if the date requested is beyond today + 1 year do nothing
	  newOffset = Math.round((newd-msd)/(1000*60*60*24));
	  document.getElementById('lengthOfStay').value = document.getElementById('lengthOfStay').value.replace(/\s+/g, "");
	  var nights = 1*document.getElementById('lengthOfStay').value;
	  var hasError = false;
	  var countAvail = 0;
 	  var countInLottery = 0;
	  for (var i = 0; i < 14; i++) {
	  	var matrixDay = document.getElementById('avail' + (i + 1));
	  	addClassName(matrixDay,'slct',false);
			if (i >= newOffset && i < (newOffset + 1*document.getElementById('lengthOfStay').value)) {
	  		addClassName (matrixDay,'slct',true);
	  		if (!hasClassName (matrixDay,'a') && !hasClassName (matrixDay,'b') && !hasClassName (matrixDay,'l') ){
	  			hasError = true;
	  		}	
				if ( hasClassName(matrixDay,'a') || hasClassName(matrixDay,'b')) { 
					countAvail =  countAvail + 1;
				} else if ( hasClassName(matrixDay,'l') ) {
					countInLottery = countInLottery  + 1;
				}		
			}
			if (i < newOffset && i >= (newOffset + 1*document.getElementById('lengthOfStay').value))  {
		  		addClassName (matrixDay,'/',false);
				//matrixDay.className = 'status a';
			}
	  }
		if (countAvail > 0 && countInLottery > 0){
			hasError = true;
			countAvail = 0;
			countInLottery = 0;
		}
	  
	  setDepartureDate(dayUseStayAdjust);
      if (document.getElementById('AlternativeSites') && date != -1) {
	  	reloadSitesDropdown(newd);
      }
	
      if (hasError) {
    	  var avMsgDiv = document.getElementById('availabilityMessages');
    	  if (avMsgDiv && avMsgDiv.innerHTML != '') {
      		avMsgDiv.style.display = "block";
    	  }
	    	if (document.getElementById('noAvailabilityMsg'))
	    		document.getElementById('noAvailabilityMsg').style.display = "block";
	      	var myLink = document.getElementById('availNotifLink');
	      	if (myLink != null) {
		      	myLink.setAttribute("href", "/viewAvailabilityNotificationDetail.do?arvdate=" + asDoubleDigit(newd.getMonth()+1)+"/"+asDoubleDigit(newd.getDate())+"/"+newd.getFullYear()
		      			+ "&lengthOfStay=" + document.getElementById('lengthOfStay').value);
	      	}
      } else {
				if (document.getElementById('availabilityMessages'))
					document.getElementById('availabilityMessages').style.display = "none";
				if (document.getElementById('noAvailabilityMsg'))
					document.getElementById('noAvailabilityMsg').style.display = "none";
      }
      
	  addClassName (document.getElementById('lengthOfStay'),'error',hasError);
	  addClassName (document.getElementById('arrivaldate'),'error',hasError);
	
	  if (document.getElementById('matrixHasError'))
		document.getElementById('matrixHasError').value = hasError?"true":"false";
	  if (document.getElementById('matrixprogresspopup'))
		document.getElementById('matrixprogresspopup').style.display = "none";
	}
	else {
		document.getElementById('matrixHasError').value = "true";
		if (document.getElementById('matrixprogresspopup'))
			document.getElementById('matrixprogresspopup').style.display = "none";
		if (document.getElementById('progresspopup'))
			document.getElementById('progresspopup').style.display ="none";
	}
}
function setDepartureDate(dayUseStayAdjust)
{
	SiteDetailsAjaxUtils.getRates();
	var dd = document.getElementById('depdate').firstChild;
	if (document.getElementById('arrivaldate').value == "") {
		dd.data = "";
		return;
	}
	var depDate = new Date(document.getElementById('arrivaldate').value);
	//Handle special case for multiple language, Ex: mer. 8 déc. 2021
	if('Invalid Date' == depDate){
		try{
			depDate = new Date(changeLocaleDateString2EnglishDateString(document.getElementById('arrivaldate').value));
		}catch(e){}
	}
	var nights  = 1*document.getElementById('lengthOfStay').value;
	depDate.setDate(1*(depDate.getDate())+(nights-dayUseStayAdjust));
	if ( !(isNaN(depDate.getYear()) || isNaN(depDate.getMonth()) || isNaN(depDate.getDate())) ) {
		dd.data = toDateString(depDate);
	} else {
		dd.data = "";
	}
}

function updateMarinaSeasonStay(endDate) {
	document.getElementById('arrivaldate').value=toDateString(readDate(document.getElementById('arrivaldate').value));
	endDate = new Date(endDate);
	newd = new Date(document.getElementById('arrivaldate').value);
	lastDate = getMaximumWindowDate();
	today = new Date();
	today.setHours(0,0,0,0);
	
	if (newd >= today && newd <= lastDate) { // if the date requested is beyond today + 1 year do nothing
		var days = Math.round((endDate - newd)/(1000*60*60*24)) + 1;
		document.getElementById('lengthOfStay').value = days;
	}
    if ($j('#arrivaldate').val()) {
   	  $j('#btnbookdates').prop('disabled', false);
    } else{
      $j('#btnbookdates').prop('disabled', true);
    }
}
function reloadSitesDropdown(newDate)
{
	var url = "/campsiteDetails.do?action=rebuildSitesDropdown&mode=changeReservationDates" ;
	var ajax = new Ajax.DynamicHTML(url);
	ajax.submit("&reservationNumber=" + document.getElementById('reservationNumber').value + 
			"&contractCode=" + document.getElementById('contractCode').value + "&siteId=" + document.getElementById('siteId').value + "&parkId=" + document.getElementById('parkId').value +  
			"&arrivaldate=" + toDateString(newDate) + "&lengthOfStay=" + document.getElementById('lengthOfStay').value);
	return false;
}

function setNewArrivalDate(matrixStartDate, newOffset, dayUseStayAdjust)
{
	var d = Date.parse (matrixStartDate);
	msd = new Date(d);
	msd.setDate(1*(msd.getDate())+newOffset);
	document.getElementById('arrivaldate').value = toDateString(msd);
	fireEvent(document.getElementById('arrivaldate'),'onchange');
	setDepartureDate(dayUseStayAdjust);
}
var WindowObjectReference;
/* Declaring a global variable which will store
 a reference to the new window to be created */
function confirmation (theUrl, confirmText) 
{
	confirmText = confirmText.replace(new RegExp(/zzz/g), "\n");
	var answer = confirm(confirmText)
	if (answer) {
		pageTracker._trackPageview("/outgoing/CrossoverToRecreationDotGov");
		theUrl = theUrl + "&arvdate=" + document.getElementById('arrivaldate').value + "&lengthOfStay=" + document.getElementById('lengthOfStay').value;
		
		if( !!window.ActiveXObject || "ActiveXObject" in window ) {
			var crossLink = document.createElement("a");
			crossLink.href = theUrl;
			crossLink.target = "_blank";
			crossLink.style = "display:none";
			document.body.appendChild(crossLink);
			crossLink.click();
		} else {
			var tools = "resizable=yes,toolbar=yes,location=yes,scrollbars=yes,menubar=yes,status=yes";
			WindowObjectReference = window.open(theUrl, 'recreation', tools);
		}
	}
	return false;	
}
function submitHandler(form, crossOver)
{
	var checkdate = true;
	var arrivaldate = document.getElementById('arrivaldate');
	var arvdate = document.getElementById('arvdate');
	if (arrivaldate.value != "") {
		var d = Date.parse(arrivaldate.value);
		var arrDate = new Date(d);
		//Handle special case for multiple language, Ex: mer. 8 déc. 2021
		if('Invalid Date' == arrDate){
			try{
				arrDate = new Date(changeLocaleDateString2EnglishDateString(arrivaldate.value));
			}catch(e){}
		}
		if (isNaN(arrDate.getYear()) ||isNaN(arrDate.getMonth()) || isNaN(arrDate.getDate())) return false;
	} else checkdate = false;
	if (checkdate == true) { 
		arvdate.value = asDoubleDigit(arrDate.getMonth()+1)+"/"+asDoubleDigit(arrDate.getDate())+"/"+arrDate.getFullYear();
	}
	var setdt = toDateString(arrDate);;
	if (setdt != arrivaldate.value) {
		alert("Your request could not be processed. Please retry or try to use an alternate browser.");
		return false;
	}
			
	if (crossOver) {
		form.target="_mywindow";
		return true;
	}
	if (submitted-- < 0) return false;
	document.getElementById('btnbookdates').style.display ="none";
	document.getElementById('progresspopup').style.display ="";
	var textMessage = 'Booking the dates ...';
	if (document.getElementById('dateChosen').value != "true") {
		textMessage = 'Checking Availability...';
	}
	showProgressBar('progresspopup',textMessage,null,'csitedetailaction');

	return (true);
}
/** 
 * Delete a Cookie 
 * @param key name of the cookie 
 */ 
function deleteCookie(key, path) 
{ 
  // Delete a cookie by setting the date of expiry to yesterday 
  date = new Date(); 
  date.setDate(date.getDate() -1);
  cookiePath = "";
  if (path && path != "") {
	  cookiePath = ';path=' + path; 
  }
  document.cookie = escape(key) + '=;expires=' + date + cookiePath;
} 

var convertedArrivalDate = false;
function showMatrixProgressPopup(matrixStartDate, date, dayUseStayAdjust, url)
{
	if( convertedArrivalDate){
		 return;
	} else {
		if( document.getElementById('arrivaldate') != ''){
			document.getElementById('arrivaldate').value=toDateString(readDate(document.getElementById('arrivaldate').value));
		}
		convertedArrivalDate = true;
	}
	if (date == null || date == "" || document.getElementById('lengthOfStay').value == null || document.getElementById('lengthOfStay').value == "") {
		convertedArrivalDate = false;		
		return;
	}
	lastDate = getMaximumWindowDate();
	today = new Date();
	today.setHours(0,0,0,0);
	newd = new Date(date);
	//Handle special case for multiple language, Ex: mer. 8 déc. 2021
	if('Invalid Date' == newd){
		try{
			newd = new Date(changeLocaleDateString2EnglishDateString(date));
		}catch(e){}
	}
	if (newd >= today && newd <= lastDate) { // if the date requested is beyond today + 1 year do nothing
		clearDepDate();
		var arrDate = new Date(document.getElementById('arrivaldate').value);
		//Handle special case for multiple language, Ex: mer. 8 déc. 2021
		if('Invalid Date' == arrDate){
			try{
				arrDate = new Date(changeLocaleDateString2EnglishDateString(document.getElementById('arrivaldate').value));
			}catch(e){}
		}
		if ( !(isNaN(arrDate.getYear()) || isNaN(arrDate.getMonth()) || isNaN(arrDate.getDate())) ) {
			msd = new Date(matrixStartDate);
			newOffset = Math.round((newd-msd)/(1000*60*60*24));
			if (document.getElementById('matrixprogresspopup') && (newOffset<0 || newOffset>=14)) {
				document.getElementById('matrixprogresspopup').style.display ="block";
			}
			//window.setTimeout("changeDate('"+ matrixStartDate+"','"+date+"','"+dayUseStayAdjust+"','"+url+"')", 1);
			changeDate(matrixStartDate, date, dayUseStayAdjust, url);
		}
	}
	else {
		document.getElementById('matrixHasError').value = "true";
		if (document.getElementById('matrixprogresspopup'))
			document.getElementById('matrixprogresspopup').style.display = "none";
		if (document.getElementById('progresspopup'))
			document.getElementById('progresspopup').style.display ="none";
	}
	changeNights(matrixStartDate, -1, dayUseStayAdjust);
	convertedArrivalDate = false;		
}

function addLengthOfStay(linkId)
{
	document.getElementById('matrixprogresspopup').style.display ="block";
	document.getElementById(linkId).href = document.getElementById(linkId).href + '&lengthOfStay=' + document.getElementById('lengthOfStay').value;
}

function clearDepDate() {
  if (document.getElementById('arrivaldate')) {
	if (document.getElementById('arrivaldate').value == "") {
		if (document.getElementById('depdate')) {
			var dd = document.getElementById('depdate').firstChild;
			dd.data = "";
		}
	}
  }
}
clearDepDate();

SiteDetailsAjaxUtils = {
		serviceURL : "/ajax/SiteDetailsAjaxUtils",
		lastArvDate : null,
		lastStay: null,
		getRates : function() {
			var arrivalDate = $j("#arrivaldate").val();
			var stay = $j("#lengthOfStay").val();
			var hasMarina = $j("#hasMarina").val();
			if (arrivalDate == SiteDetailsAjaxUtils.lastArvDate && stay == SiteDetailsAjaxUtils.lastStay) {
				return;
			}
			SiteDetailsAjaxUtils.lastArvDate = arrivalDate;
			SiteDetailsAjaxUtils.lastStay = stay;
			var siteId = $j("#siteId").val();
			var parkId = $j("#parkId").val();
			var contractCode = $j("#contractCode").val();
			req = {
				action : "GET_RATES",
				siteId : siteId,
				parkId : parkId,
				contractCode : contractCode,
				arrivalDate : arrivalDate,
				stay: stay,
				hasMarina: hasMarina
			};

			$j.ajax({
				url : SiteDetailsAjaxUtils.serviceURL + "?"
						+ $j.param(req, true),
				type : 'POST',
				contentType : 'application/json',
				success : function(response) {
					obj = $j.parseJSON(response);
					$j("#rateTable").replaceWith(obj.rateSection);
					SiteDetailsAjaxUtils.onSuccessGetRates(response);
				},
				error : function() {
					SiteDetailsAjaxUtils.onErrorGetRates();
				}
			});
		},

		onSuccessGetRates : function(data) {
		},

		onErrorGetRates : function() {
		},

}

</script>

	  		</div>
	  		
			</div>
	  </td>



     <!-- coloumn 3 -->

</tr>

</table>
</div>
